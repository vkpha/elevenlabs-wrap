{
  "name": "elevenlabs-wrap",
  "version": "1.0.0",
  "description": "ElevenLabs Wrapped - Year-end listening experience with AI-generated music",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently --names \"API,WEB\" --prefix-colors \"cyan,magenta\" \"npm run dev:api\" \"npm run dev:web\"",
    "dev:api": "npm start --workspace=@elevenlabs-wrap/api",
    "dev:web": "npm run dev --workspace=@elevenlabs-wrap/web",
    "start:api": "npm start --workspace=@elevenlabs-wrap/api",
    "start:web": "npm start --workspace=@elevenlabs-wrap/web"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cors": "^2.8.5"
  },
  "keywords": [
    "elevenlabs",
    "wrapped",
    "music",
    "ai"
  ],
  "author": "",
  "license": "ISC"
}
